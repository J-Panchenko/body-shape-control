{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
		"plugin:react/recommended",
		"airbnb"
	],
	"parser": "@babel/eslint-parser",
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 12,
		"sourceType": "module",
		"requireConfigFile": false,
		"babelOptions": {
			"presets": [
				"@babel/preset-react"
			]
		}
	},
	"plugins": [
		"react"
	],
	"rules": {
		"react/prop-types": "off",
		"react/jsx-filename-extension": [
			1,
			{
				"extensions": [
					".js",
					".jsx"
				]
			}
		],
		"react/jsx-props-no-spreading": "off",
		"quote-props": "off",
		"jsx-a11y/label-has-associated-control": "off"
	}
}